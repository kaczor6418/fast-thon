{
  "name": "fast-thon",
  "version": "0.0.1",
  "description": "App boilerplate for hackathons. React + MUI + Firebase",
  "sideEffects": false,
  "source": "src/main.tsx",
  "keywords": [
    "hacathon",
    "boilerplate"
  ],
  "author": "Krzysztof Kaczy≈Ñski",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/kk-js/kk-utils#readme",
  "bugs": {
    "url": "https://github.com/kaczor6418/fast-thon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaczor6418/fast-thon.git"
  },
  "scripts": {
    "postinstall": "git config core.hooksPath ./git-hooks",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --cache --ext .ts --ext .tsx",
    "lint:fix": "eslint . --cache --ext .ts --ext .tsx --fix",
    "lint:watch": "esw . --watch --cache --ext .ts --ext .tsx ",
    "prettyprint": "prettier --check .",
    "prettyprint:fix": "prettier --write .",
    "firebase:link-app": "swc -C jsc.parser.syntax=typescript ./scripts/replace-firebase-project-id.mts -o temp-replace.mjs && node temp-replace.mjs",
    "firebase:login": "firebase login",
    "firebase:start:emulators": "cd firebase && firebase emulators:start --import=./seed --export-on-exit",
    "firebase:deploy": "cd firebase && firebase deploy",
    "firebase:deploy:hosting": "cd firebase && firebase deploy --only hosting",
    "firebase:deploy:storage-rules": "cd firebase && firebase deploy --only storage",
    "firebase:deploy:firestore-rules": "cd firebase && firebase deploy --only firestore:rules",
    "firebase:deploy:firesotre-indexes": "cd firebase && firebase deploy --only firestore:indexes",
    "git:stop-tracking-db-seed": "git rm -r --cached firebase/seed/",
    "test:integration": "jest /test/integration --testEnvironment=node --passWithNoTests --setupFilesAfterEnv ./test/integration/integration-setup.ts",
    "test:integration:watch": "jest /test/integration --testEnvironment=node --passWithNoTests --setupFilesAfterEnv ./test/integration/integration-setup.ts --watch",
    "ci:test:integration": "cd firebase && firebase emulators:exec 'jest /test/integration --testEnvironment=node --coverage=false --passWithNoTests --setupFilesAfterEnv ./test/integration/integration-setup.ts'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-router": "^1.150.0",
    "d": "^1.0.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "firebase": "^12.8.0",
    "i18next": "^25.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@tanstack/react-router-devtools": "^1.150.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "firebase-tools": "^15.3.1",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "^6.0.4"
  }
}
